server {
	listen 443 ssl;
	listen [::]:443 ssl;

	server_name daortega.42.fr;

	root /var/www/daortega.42.fr;
	index index.html;

	ssl_certificate /etc/ssl/certs/daortega.crt;
	ssl_certificate_key /etc/ssl/private/daortega.key;	
	
	ssl_protocols TLSv1.3;

	location / {
		try_files $uri $uri/ =404;
	}
}
